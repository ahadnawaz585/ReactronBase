{
  "name": "Reactron Base",
  "version": "1.0.0",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "webpack serve --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "build:db": "tsc src/db/seeder.ts && tsc src/db/reset.ts",
    "package": "electron-packager . --platform=win32 --arch=x64 --out=dist --overwrite",
    "seed": "npm run build:db && node dist/db/seeder.js",
    "force-seed": "npm run build:db && node dist/db/seeder.js --force",
    "reset": "npm run build:db && node dist/db/reset.js",
    "electron": "electron ./dist/main/index.js"
  },
  "keywords": [],
