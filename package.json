{
  "name": "reactron-base",
  "version": "1.0.1",
  "main": "dist/main/index.js",
  "scripts": {
    "build:main": "tsc src/main/index.ts --outDir dist/main",
    "build:renderer": "vite build",
    "build": "npm run build:renderer && npm run build:main",
    "dev": "cross-env NODE_ENV=development concurrently \"vite\" \"electron .\"",
    "package": "electron-packager . --platform=win32 --arch=x64 --asar",
    "seed": "npm run build:db && node dist/db/seeder.js",
    "force-seed": "npm run build:db && node dist/db/seeder.js --force",
    "reset": "npm run build:db && node dist/db/reset.js",
    "electron": "electron ./dist/main/index.js"
  },
  "keywords": [],
  "author": "",
  "repository": {
    "url": "https://github.com/ahadnawaz585/ReactronBase"
  },
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/react-router": "^5.1.20",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^32.0.1",
    "electron-packager": "^17.1.2",
    "electron-reload": "^2.0.0-alpha.1",
    "postcss-loader": "^8.1.1",
    "prisma": "^5.18.0",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-css": "^1.0.4",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-sass": "^0.1.0"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@prisma/client": "^5.18.0",
    "electron-is-dev": "^3.0.1",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
