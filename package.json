    "@types/node": "^22.5.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "electron": "^32.0.1",
    "electron-packager": "^17.1.2",
    "electron-reload": "^2.0.0-alpha.1",
